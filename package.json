{
  "name": "react-starter-2019",
  "version": "1.0.0",
  "description": "My webpack project",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deepanrajkumar/react-starter-2019.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/deepanrajkumar/react-starter-2019/issues"
  },
  "homepage": "https://github.com/deepanrajkumar/react-starter-2019#readme",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "json-server": "json-server --watch config/db.json",
    "dev": "parcel src/index.html",
    "dev:mock": "cross-env PET_MOCK=mock parcel src/index.html",
    "format": "prettier --write \"**/*.{js,jsx,css,scss,sass,json}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "lint:all": "eslint \"**/*.{js,jsx}\" --fix",
    "test": "jest",
    "build": "webpack",
    "watch": "webpack --watch",
    "start": "webpack-dev-server --open",
    "server": "node server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,css,scss,sass,json,md,html}": [
      "prettier --write",
      "git add"
    ],
    "*.{scss,sass}": [
      "sass-lint-auto-fix",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/dom": "^6.3.0",
    "@testing-library/jest-dom": "^4.1.0",
    "@testing-library/react": "^9.1.4",
    "@testing-library/user-event": "^7.0.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.0",
    "csv-loader": "^3.0.2",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "express": "^4.17.1",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "json-server": "^0.15.1",
    "lint-staged": "^9.2.5",
    "lodash": "^4.17.15",
    "node-sass": "^4.12.0",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.9.0",
    "sass-lint": "^1.13.1",
    "sass-lint-auto-fix": "^0.17.0",
    "sass-loader": "^7.3.1",
    "semantic-git-commit-cli": "^3.3.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-middleware": "^3.7.1",
    "webpack-dev-server": "^3.8.1",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1"
  }
}
