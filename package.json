{
  "name": "react-starter-2019",
  "version": "1.0.0",
  "description": "React strater kit",
  "repository": {
    "type": "git",
    "url": "git@github.com:deepanrajkumar/react-starter-2019.git"
  },
  "author": "Deepan raj kumar",
  "license": "ISC",
  "main": "src/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=12.13.0"
  },
  "sasslintConfig": ".sass-lint.yml",
  "staticPath": [
    "build_scripts"
  ],
  "scripts": {
    "format": "prettier --write \"**/*.{js,jsx,css,scss,sass,json}\"",
    "eslint": "eslint \"src/**/*.{js,jsx}\"",
    "eslint:all": "eslint \"**/*.{js,jsx}\" --fix",
    "eslint:fix": "eslint \"src/**/*.{js,jsx}\" --fix",
    "scan": "npx hawkeye scan",
    "sass:lint": "sass-lint -v \"src/**/*.scss\"",
    "sass:lint-check": "sass-lint -v -q \"src/**/*.scss\"",
    "clean:dist": "rimraf dist",
    "clean:all": "rimraf dist .cache coverage __test__/coverage node_modules package-lock.json yarn.lock",
    "commit": "sgc",
    "check": "npm run eslint && npm run sass:lint",
    "watch": "nodemon --exec \"npm run check\"",
    "commit:check": "npm run format && npm run eslint && npm run sass:lint && npm run fix && npm run test:coverage && npm run scan",
    "build:check": "npm run eslint && npm run test:prod && npm run build",
    "precommit:check": "npm run format && npm run eslint && npm run sass:lint",
    "fix": "npm run format && npm run eslint:fix",
    "json-server": "json-server -r config/routes.json -w config/db.json --host 0.0.0.0",
    "test": "jest --config config/jest.config.js",
    "test:watch": "jest --watchAll --config config/jest.config.js",
    "test:coverage": "jest --coverage --collectCoverage --config config/jest.config.js",
    "test:prod": "jest --config \"./config/jest.config.prod.js\"",
    "build": "npm run clean:dist && webpack --mode=production --config config/webpack.prod.js",
    "build:prod": "webpack --mode=production --config config/webpack.prod.js",
    "dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --colors",
    "dev:local": "concurrently \"npm run json-server\" \"npm run dev\"",
    "start": "npm run dev",
    "start:build": "npm run serve:build",
    "start:dev": "concurrently \"npm run watch\" \"npm run dev\"",
    "start:dev:local": "concurrently \"npm run json-server\" \"npm run watch\" \"npm run dev\"",
    "serve:build": "npm run build && node buildServer.js",
    "serve:dev": "node config/devServer.js",
    "serve:dev:local": "concurrently \"npm run json-server\" \"npm run serve:dev\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{js,jsx,css,scss,sass,json,md,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.6.4",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@hawkeyesec/scanner-cli": "^1.7.1",
    "@testing-library/react": "^9.3.2",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "csv-loader": "^3.0.2",
    "dotenv-webpack": "^1.7.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^4.2.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "jest-fetch-mock": "^2.1.2",
    "json-server": "^0.15.1",
    "lint-staged": "^9.4.2",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.11.0",
    "regenerator-runtime": "^0.13.3",
    "rimraf": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^8.0.0",
    "semantic-git-commit-cli": "^3.5.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.2",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "webpack-merge": "^4.2.2",
    "xml-loader": "^1.2.1"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-router-dom": "^5.1.2"
  }
}
